{
    "compilerOptions": {
        "strict": true,
        "lib": ["ES2022", "DOM", "DOM.Iterable"],
        "module": "ESNext",
        "noEmit": true,
        "skipLibCheck": true,
        "sourceMap": false,
        "declaration": false,
        "declarationMap": false,
        "target": "ES2022",
        "isolatedModules": true,
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true
    },
    "exclude": [".git", "node_modules"]
}
